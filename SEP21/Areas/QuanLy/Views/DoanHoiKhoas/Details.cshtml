@model SEP21.Models.DoanHoiKhoa

@{
    ViewBag.Title = "Details";
    Layout = "~/Areas/QuanLy/Views/Shared/_AdminLayout.cshtml";
}

<h2>Chi Tiết </h2>

<div>
    <h4>Đoàn Hội Khoa</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Chức Vụ
        </dt>
        <dd>
            @Html.DisplayFor(model => model.ChucVu)
        </dd>
        <dt>
            Đoàn
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Doan)
        </dd>
        <dt>
            Hội
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Hoi)
        </dd>
        <dt>
            MSSV
        </dt>
        <dd>
            @Html.DisplayFor(model => model.SinhVien.MSSV)
        </dd>
        <dt>
            Hình Ảnh
        </dt>
    </dl>
    <div class="form-group">
        <label class="control-label col-md-2" for="IMG"></label>
        <img id="Picture" src="@Url.Action("Picture", new { Model.ID })" />
    </div>
</div>
<p>
    @Html.ActionLink("Chỉnh Sửa", "Edit", new { id = Model.ID }) |
    @Html.ActionLink("Trở Lại", "Index")
</p>
