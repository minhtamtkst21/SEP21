@model SEP21.Models.NCKH

@{
    ViewBag.Title = "Delete";
    Layout = "~/Areas/QuanLy/Views/Shared/_AdminLayout.cshtml";
}

<h2>Xóa</h2>

<h3>Bạn Chắc Chắn Muốn Xóa Không ?</h3>
<div>
    <h4>Nghiên Cứu Khoa Học</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Tên
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Ten)
        </dd>
        <dt>
            Nội Dung
        </dt>
        <dd>
            @Html.DisplayFor(model => model.NoiDung)
        </dd>
    </dl>
    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()
        <div class="form-actions no-color">
            <input type="submit" value="Xóa" class="btn btn-default" /> |
            @Html.ActionLink("Trở Lại", "Index")
        </div>
    }
</div>
